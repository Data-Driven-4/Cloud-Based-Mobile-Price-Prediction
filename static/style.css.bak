body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
}

h2 {
    text-align: center;
    margin-top: 20px;
}

#resultContainer {
    max-width: 600px;
    margin: 20px auto;
    padding: 20px;
    background-color: #f9f9f9;
    border-radius: 10px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
}

#predictedPriceRange {
    font-weight: bold;
}

#inputFeatures {
    list-style-type: none;
    padding: 0;
}

#inputFeatures li {
    margin-bottom: 10px;
}

#inputFeatures li span {
    font-weight: bold;
}
